{% extends 'base.html.twig' %}

{% block title %}Conference Guestbook{% endblock %}

{% block body %}
    <div class="example-wrapper">
        <h2>Give your feedback!</h2>
        {% for conference in conferences %}
            <h4>{{ conference }}</h4>
            <p>
                <a href="{{ path('app_conference', {slug : conference.slug}) }}">View</a>
            </p>
        {% endfor %}
    </div>
{% endblock %}
